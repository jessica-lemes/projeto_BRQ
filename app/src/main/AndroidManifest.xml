        android:supportsRtl="true"
        android:theme="@style/Theme.App_BRQ">
        <activity android:name=".UI.DespesasActivity" android:theme="@style/Theme.Despesas"/>
        <activity android:name=".UI.ReceitasActivity" android:theme="@style/Theme.Receitas"/>
        <activity android:name=".UI.PrincipalActivity">
